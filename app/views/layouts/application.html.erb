<!DOCTYPE html>
<html>
<head>
  <title>Ratebeer</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="navibar">
    <button type="button"><%= link_to 'Breweries', breweries_path %></button>
    <button type="button"><%= link_to 'Beers', beers_path %></button>
    <button type="button"><%= link_to "Ratings", ratings_path%></button>
  </div>

  <%= yield %>

</body>
</html>
