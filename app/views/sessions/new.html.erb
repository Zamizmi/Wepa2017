<p id="notice"><%= notice%></p>

<h1>Sign in</h1>

<%= form_tag session_path do %>
<label for=":username">Username</label><br> <%= text_field_tag :username, params[:username] %><br>
<label for=":password">Password</label><br> <%= password_field_tag :password, params[:password]%>
<br>
<br>
<%= submit_tag "Log in"%>
<% end %>
