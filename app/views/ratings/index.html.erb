<p id="notice"><%= notice %></p>

<h2>List of ratings</h2>
<p>Number of ratings: <%=@ratings.count%></p>

<ul>
  <% @ratings.each do |rating| %>
    <li> <%=rating %> <button type="button"> <%= link_to 'Delete', rating_path(rating.id), method: :Delete, data: { confirm: 'Are you sure?' } %></button> </li>
    <%end%>
</ul>

<%= link_to "New Rating", new_rating_path%>
